{
	"name": "access_dedicated_sqlpool",
	"properties": {
		"description": "you have to define a spark temporary table in scala for pyspark to access it",
		"nbformat": 4,
		"nbformat_minor": 2,
		"bigDataPool": {
			"referenceName": "asaSpark",
			"type": "BigDataPoolReference"
		},
		"sessionProperties": {
			"driverMemory": "28g",
			"driverCores": 4,
			"executorMemory": "28g",
			"executorCores": 4,
			"numExecutors": 2,
			"conf": {
				"spark.dynamicAllocation.enabled": "false",
				"spark.dynamicAllocation.minExecutors": "2",
				"spark.dynamicAllocation.maxExecutors": "2"
			}
		},
		"metadata": {
			"saveOutput": true,
			"kernelspec": {
				"name": "synapse_spark",
				"display_name": "Synapse Spark"
			},
			"language_info": {
				"name": "python"
			},
			"a365ComputeOptions": {
				"id": "/subscriptions/1fb20658-4607-4052-a198-e277fe0ebf24/resourceGroups/Frank-Azure-Analytics/providers/Microsoft.Synapse/workspaces/asaws-frank/bigDataPools/asaSpark",
				"name": "asaSpark",
				"type": "Spark",
				"endpoint": "https://asaws-frank.dev.azuresynapse.net/livyApi/versions/2019-11-01-preview/sparkPools/asaSpark",
				"auth": {
					"type": "AAD",
					"authResource": "https://dev.azuresynapse.net"
				},
				"sparkVersion": "2.4",
				"nodeCount": 10,
				"cores": 4,
				"memory": 28,
				"automaticScaleJobs": false
			},
			"sessionKeepAliveTimeout": 30
		},
		"cells": [
			{
				"cell_type": "code",
				"metadata": {
					"microsoft": {
						"language": "scala"
					}
				},
				"source": [
					"%%spark\n",
					"val df = spark.read.synapsesql(\"asadedicated.asacourse.customers\") \n",
					"// df.write.mode(\"overwrite\").saveAsTable(\"default.t1\")\n",
					"// display(df)\n",
					"df.registerTempTable(\"pyspark_customers\")"
				],
				"execution_count": 5
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"df = spark.sql('SELECT * FROM pyspark_customers') \r\n",
					"display(df)"
				],
				"execution_count": 6
			}
		]
	}
}